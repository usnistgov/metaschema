    16777348, 16777348, 16777348, 329900, 329900, 17107116, 0, 4, 0, 8, 0, 0, 0, 2048, 0, 2048, 8192, 0, 4, 32, 128, 2048, 65536, 262144, 0, 0, 0, 640, 0, 4,
    128, 0, 40, 0, 4, 32, 2048, 65536, 262144, 8, 0, 0, 2048, 0, 0, 8, 32, 8, 0, 2048, 0, 2097152, 2048, 0, 2048, 0, 0, 2048, 2048, 2048, 2048, 33554432,
    268435456, 0, 0, 8192, -2147483648, 3145728
