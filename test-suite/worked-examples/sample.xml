
<group base=".">
<work id="nist-sp-800-53rev4">
  <title>NIST SP 800-53 (revision 4) Control Catalog</title>
  <validate rules="oscal_catalog_schema"/>
  <validate rules="nist-sp800-53_catalog_schematron"/>
  <item format="xml"  href="....">
    <valid timestamp="{when}" href="oscal_catalog_schema.xsd" />
  </item>  
  <!--prefer, fallback
  file, address, proxy-->
  
  
  <!-- Use json-value-key here! -->
  <item format="json" href="...."/>
  <item format="xml" href="...."/>
  <item format="json" href="...."/>
  <mirror base="remote-base"/> <!-- implies duplicate -->
  <mirror base="remote-base">
    <item format="json" href="...."/>
    <item format="xml" href="...."/>
  </mirror>	
</work>
<work id="oscal_catalog_schema">
  <item format="xml"  href="....">
    <valid ruleset="XSD Schema"/>
	</item>
  <item format="json" href="...."/>
</work>
<work id="FedRAMP_MODERATE_profile">
    <valid ruleset="oscal_profile_schema"/>
  <item format="xml"  href="...."/>
  <item format="json" href="...."/>
</work>
<work id="FedRAMP_MODERATE_profile_resolved">
    <valid ruleset="oscal_catalog_schema" />
    <valid ruleset="nist-sp800-53_catalog_schematron"/> 
  <item format="xml"  href="...."/>
  <item format="json" href="...."/>
</work>

</group>
