                     <m:description>Has a local definition</m:description>
                  </xs:appinfo>
                  <xs:documentation>
                     <b>Field defined locally</b>: Has a local definition</xs:documentation>
               </xs:annotation>
               <xs:simpleContent>
                  <xs:extension base="xs:string"/>
               </xs:simpleContent>
            </xs:complexType>
         </xs:element>
