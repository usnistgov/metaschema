      </xs:appinfo>
   </xs:annotation>
   <xs:element name="parent" type="metaschema-group-as:parent-ASSEMBLY"/>
   <xs:complexType name="parent-ASSEMBLY">
      <xs:annotation>
         <xs:appinfo>
            <m:formal-name>parent</m:formal-name>
            <m:description>parent assembly</m:description>
